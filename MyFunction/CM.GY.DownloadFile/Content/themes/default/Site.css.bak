/*------GridView样式表--------4BA0C3*/
/*Table的整体样式*/
.grid
{
	border-collapse: collapse;
    border: 1px solid #CCCCCC;
}
/*表头的样式*/
.grid-head
{
    /*background-color: #5bc0ea;*/
    background-image:url('images/grid_head.png');
    background-position:0px,-2px;
    color: White;
    font-weight: bold;
    height: 25px;
    border: 1px solid #8f8f8f;
}
.grid-head A
{
    color: White;
    text-decoration: none;
}
/*表行的样式* */
.grid-row
{
    border: 1px solid #CCCCCC;
}

.grid-cell
{
    height: 25px;
    border: 1px solid #CCCCCC;
    padding-left:2px;
    padding-right:2px;
}

/*grid链接按钮 start*/
.grid-cell A:link
{
    COLOR: #4646a3;/*偏紫蓝色*/
    TEXT-DECORATION: none
}
.grid-cell A:visited
{
    COLOR: #9353d4;/*偏紫色*/
    TEXT-DECORATION: none;
}
.grid-cell A:hover
{
    COLOR: #47b930;/*绿色*/
    TEXT-DECORATION: none;
	text-decoration:underline;
}
.grid-cell A:active
{
    COLOR: #4646a3;/*偏紫蓝色*/
    TEXT-DECORATION: none
}
/*grid链接按钮 end*/

/*grid分页 start*/
.grid-pager
{
	COLOR: #4646a3;/*偏紫蓝色*/
    TEXT-DECORATION: none
}
.grid-pager:hover
{
	 COLOR: #47b930;/*绿色*/
    TEXT-DECORATION: none;
	text-decoration:underline;
}
/*grid分页 end*/


/*表交替行的样式*/
.grid-row-alternate
{
    background-color: #d2eaf8;
}
/*表选中行的样式*/
.grid-row-selected
