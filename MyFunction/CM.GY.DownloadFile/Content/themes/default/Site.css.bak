/****************************************表单按钮end****************************************/

.tablerange 
{
	margin-top:20px;
	border: #b8c9d6 1px solid; 
	border-collapse:collapse;
	border-color:#b8c9d6;
	font-size:12px;
	font:"Times New Roman", Times, serif;
	width:96%;
}
.tablerange table
{ 
	font-size: 9pt;
    border-collapse: collapse;
    margin: 0 auto;
    border: 0;
}
.tablerange td
{
	border-right: #CCCCCC 1px solid;
    border-top: #CCCCCC 1px solid;
    border-left: #CCCCCC 1px solid;
    border-bottom: #CCCCCC 1px solid;
    height:25px;
}
.tabletop 
{
	color: #003380;
	font-weight: bold;
	border:1px solid;
	text-align:center;
	background-image: url(Images/white-top-bottom.gif);
	color: navy;
	padding-left: 2px;
	background-repeat: repeat-x;
	  
}
.tabletitle 
{
	 font:"Times New Roman", Times, serif;
	  text-align:center; 
	  width:96%; 
	  font-size:14px; 
	  line-height:24px; 
	  color:#6600FF; 
 }

/****************************************表单样式end******************************************/
/********************************案卷列表页面相关样式start***********************************/
.case-list
{
	font-size: 11pt;
	COLOR: #4646a3;/*偏紫蓝色*/
    TEXT-DECORATION: none
}
.case-list:hover
{
	font-size: 11pt;
	COLOR: #47b930;/*绿色*/
    TEXT-DECORATION: none;
	text-decoration:underline;
}
/********************************案卷列表页面相关样式end***********************************/

/********************************首页模块样式start***********************************/
.hometitle 
{
	background-image: url(images/hometitle.gif);
	background-repeat: repeat-x;
	font-family:微软雅黑;
	font-weight:bold;
	color:#0060c1;
	font-size:14px;
}
.hometitle2
{
	background-image: url(images/hometitle.gif);
	background-repeat: repeat-x;
	font-family:宋体;
	color:#0060c1;
	font-size:12px;
	vertical-align:bottom;
	padding-bottom:3px;
}
.homeborder 
{
	border: 1px solid #8eb4ce;
	background-color:White;
}
.homedata:link
{
    color:#7a7a7a;
	TEXT-DECORATION: none
}
.homedata:visited
{
    color:#7a7a7a;
	TEXT-DECORATION: none
}
.homedata:hover
{
	COLOR: #911104;
	TEXT-DECORATION: underline
}
/********************************首页模块样式end***********************************/

/********************************老页面样式***********************************/
.TABLE1
{
    font-size: 9pt;
    border-collapse: collapse;
    margin: 0 auto;
    border: 0;
}
/*放控件的td*/
.TD2
{
    border-right: #CCCCCC 1px solid;
    border-top: #CCCCCC 1px solid;
    border-left: #CCCCCC 1px solid;
    border-bottom: #CCCCCC 1px solid;
}
/*放名称的td*/
.TD1
{
    border-right: #CCCCCC 1px solid;
    border-top: #CCCCCC 1px solid;
    border-left: #CCCCCC 1px solid;
    border-bottom: #CCCCCC 1px solid;
    background-color: #EFF4FA;
}
.tbButton
{
    width: 73px;
    height: 22px;
    background-image: url(images/btnmiddle1.png);
    border: none;
    cursor: hand;
    background-color: transparent;
}
.InputBox
{
    border-right: #CCCCCC 1px solid;
    border-top: #CCCCCC 1px solid;
    border-left: #CCCCCC 1px solid;
    border-bottom: #CCCCCC 1px solid;
    background-color: #f4f4f4;
    box-sizing: border-box;
    -webkit-box-sizing:border-box;
    -moz-box-sizing: border-box;
}
.headline
{
    FONT-WEIGHT: bold;
    FONT-SIZE: 12pt;
    COLOR: darkblue;
    FONT-FAMILY: Tahoma, Verdana, Arial, "新宋体", "宋体";;
    BACKGROUND-COLOR: transparent
}
.datagrid
{
	border-collapse: collapse;
    border: 1px solid #CCCCCC;
}
/*表头的样式*/
.dg_header
{
    background-color: #8BC4FB;
    /*background-image:url('images/grid_head.png');*/
    background-position:0px,-2px;
    color: White;
    font-weight: bold;
    height: 25px;
    border: 1px solid #8f8f8f;
}
.dg_header A
{
    color: White;
    text-decoration: none;
}
/*表交替行的样式*/
.dg_alter
{
    background-color: #d2eaf8;
}
/*表选中行的样式*/
.dg_item
{
	 border: 1px solid #CCCCCC;
}
/********************************老页面样式end***********************************/